{
  "name": "betterTwitter",
  "version": "0.0.1",
  "description": "A typed twitter client",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "!dist/**/*.map"
  ],
  "repository": "https://github.com/larsvommars/betterTwitter",
  "author": "LarsVomMars",
  "license": "MIT",
  "private": "true",
  "scripts": {
    "build": "./node_modules/.bin/tsc -p .",
    "watch": "./node_modules/.bin/tsc -p . -w",
    "lint": "./node_modules/.bin/eslint src/ --ext ts",
    "checkFormat": "./node_modules/.bin/prettier -c {*,src/**/*}.ts",
    "format": "./node_modules/.bin/prettier -w {*,src/**/*}.ts",
    "test": "node dist/test/test.js",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "devDependencies": {
    "@types/node": "^14.14.17",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^3.9.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^5.0.6",
    "pinst": "^2.1.1",
    "prettier": "^2.0.5",
    "typescript": "^4.1.3"
  }
}
